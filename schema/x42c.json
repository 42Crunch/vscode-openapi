          "type": "object",
          "required": ["header.name", "authorized.algorithms"],
          "properties": {
            "header.name": {
              "type": "string",
              "pattern": "^[0-9a-zA-Z\\-_]{1,100}$",
              "minLength": 1,
              "maxLength": 100
            },
            "jwk.envvar": {
              "type": "string",
              "minLength": 1,
              "maxLength": 100,
              "default": "JWK"
            },
            "forward.jwt": {
              "type": "boolean",
              "default": true
            },
            "lifetime": {
              "type": "number",
              "minimum": 0,
              "maximum": 99999999,
              "default": 5
            },
            "drift": {
              "type": "number",
              "minimum": 0,
              "maximum": 86400,
              "default": 10
            },
            "authorized.algorithms": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "HS256",
                  "HS384",
                  "HS512",
                  "RS256",
                  "RS384",
                  "RS512",
                  "PS256",
                  "PS384",
                  "PS512",
                  "ES256",
                  "ES384",
                  "ES512"

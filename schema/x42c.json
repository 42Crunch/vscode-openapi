            "jp.claims.schema": {
              "type": "string",
              "minLength": 1,
              "maxLength": 50000,
              "pattern": "^\\#(/([^/~]+|~[01])+)*$"
            },
            "claims.schema": {
              "type": "object",
              "default": {}
